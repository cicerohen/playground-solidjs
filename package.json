    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@rollup/plugin-replace": "^3.0.0",
    "babel-preset-solid": "^1.2.2",
    "dotenv": "^10.0.0",
    "eslint": "^8.2.0",
    "husky": "^7.0.4",
    "lint-staged": "^11.2.6",
    "node-sass": "^6.0.1",
    "postcss": "^8.3.11",
    "prettier": "^2.4.1",
    "rollup": "^2.59.0",
    "rollup-plugin-html2": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.5",
